<?xml version="1.0" ?>
<launch>
    <node name="controller" pkg="pid" type="controller" ns="summit_xl_a/pid_x" output="screen" >
      <param name="Kp" value="1.0" />
      <param name="Ki" value="0.1" />
      <param name="Kd" value="0.0" />
      <param name="upper_limit" value="0.5" />
      <param name="lower_limit" value="-0.5" />
      <param name="windup_limit" value="0.01" />
      <param name="cutoff_frequency" value="3" />
      <param name="diagnostic_period" value="0.25" />
      <param name="max_loop_frequency" value="50.0" />
      <param name="min_loop_frequency" value="15.0" />
     </node>
    <node name="controller" pkg="pid" type="controller" ns="summit_xl_a/pid_y" output="screen" >
      <param name="Kp" value="1.0" />
      <param name="Ki" value="0.1" />
      <param name="Kd" value="0.0" />
      <param name="upper_limit" value="0.5" />
      <param name="lower_limit" value="-0.5" />
      <param name="windup_limit" value="0.01" />
      <param name="cutoff_frequency" value="3" />
      <param name="diagnostic_period" value="0.25" />
      <param name="max_loop_frequency" value="50.0" />
      <param name="min_loop_frequency" value="15.0" />
     </node>
     <node name="controller" pkg="pid" type="controller" ns="summit_xl_a/pid_yaw" output="screen" >
      <param name="Kp" value="1.0" />
      <param name="Ki" value="0.1" />
      <param name="Kd" value="0.0" />
      <param name="upper_limit" value="0.5" />
      <param name="lower_limit" value="-0.5" />
      <param name="windup_limit" value="0.2" />
      <param name="cutoff_frequency" value="3" />
      <param name="diagnostic_period" value="0.25" />
      <param name="max_loop_frequency" value="50.0" />
      <param name="min_loop_frequency" value="15.0" />
     </node>

    <!--node name="rqt_plot_x" pkg="rqt_plot" type="rqt_plot"
    args="/summit_xl_a/pid_x/control_effort /summit_xl_a/pid_x/state /summit_xl_a/pid_x/setpoint" /-->

    <!--node name="rqt_plot_y" pkg="rqt_plot" type="rqt_plot"
    args="/summit_xl_a/pid_y/control_effort /summit_xl_a/pid_y/state /summit_xl_a/pid_y/setpoint" /-->

    <!--node name="rqt_plot_yaw" pkg="rqt_plot" type="rqt_plot"
    args="/summit_xl_a/pid_yaw/control_effort /summit_xl_a/pid_yaw/state /summit_xl_a/pid_yaw/setpoint" /-->

    <!-- node name="rqt_reconfigure" pkg="rqt_reconfigure" type="rqt_reconfigure" /-->

    <!--node pkg="position_controller" type="position_controller_agv.py"
name="position_controller" output="screen"/-->
</launch>
